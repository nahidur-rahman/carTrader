#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../listhen@1.2.2/node_modules/listhen/bin/listhen.mjs" "$@"
else
  exec node  "$basedir/../../../../../listhen@1.2.2/node_modules/listhen/bin/listhen.mjs" "$@"
fi
